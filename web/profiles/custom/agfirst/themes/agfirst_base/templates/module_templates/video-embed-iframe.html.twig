<iframe
  {{ attributes.setAttribute('title', 'Quotation Layer - Embedded Video') }}
  {% if url is not empty %}
    src="{{ url }}{% if query is not empty %}?{{ query | url_encode }}{% endif %}{% if fragment is not empty %}#{{ fragment }}{% endif %}"
  {% endif %}>
</iframe>
